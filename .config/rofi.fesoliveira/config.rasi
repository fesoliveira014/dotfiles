configuration {
  modes: [ combi ];
  combi-modes: [ window, drun, run ];
}

@theme "gruvbox-light"

entry {
    placeholder: "Type here";
    cursor: pointer;
}

element {
    orientation: horizontal;
    children: [ element-text, element-icon];
    spacing: 5px;
}

element-icon {
    size: 2.5em;
}

entry, element-text {
    text-color: white;
    background-color: rgb(24, 24, 24);
}

element-text {
    vertical-align: 0.5;
}